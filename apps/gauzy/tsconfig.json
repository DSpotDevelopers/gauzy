{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "../../dist/out-tsc",
		"target": "ES2022",
		"module": "es2020",
		"downlevelIteration": true,
		"useDefineForClassFields": false
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": false,
		"strictTemplates": false,
		"strictInjectionParameters": false
	},
	"include": [
		"**/*.ts",
		"**/workers/*.js"
	],
	"exclude": [
		"jest.config.ts"
	]
}
